{
	"Name": "Querschnitt",
	"Description": "Dev tool",
	"LoadPriority": 1,
	"RequiredOnClient": true,
	"Version": "1.0.0",
	"Dependencies": {
		"CLIENT_HAS_UEBERBLICK": "Ueberblick"
	},
	"Scripts": [
		{
			"Path": "ueberblick_2.nut",
			"RunOn": "CLIENT || SERVER || UI",
