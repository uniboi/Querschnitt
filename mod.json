			}
		},
		{
			"Path": "sh_devtools.nut",
			"RunOn": "SERVER || CLIENT || UI"
		},
		{
			"Path": "t_scripts.nut",
			"RunOn": "SERVER || CLIENT || UI"
		},
		{
			"Path": "weapons/mp_dev_info_gun.nut",
			"RunOn": "CLIENT || SERVER",
			"ClientCallback": {
				"After": "DevInfoGunInit"
			},
			"ServerCallback": {
				"After": "DevInfoGunInit"
			}
		},
		{
			"Path": "sh_networked_devtools.nut",
			"RunOn": "CLIENT || SERVER",
			"ClientCallback": {
				"Before": "DevTools_Network"
			},
			"ServerCallback": {
				"Before": "DevTools_Network"
			}
		},
